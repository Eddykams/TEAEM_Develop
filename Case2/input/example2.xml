<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration>
<goals name="Business Goals" type="AND" >  
	<goal name="GDPR Compliance" />
	<goal name="Auditability" /> 
</goals>

    <mapping>   
			<goal name="Public BC" mappedFeature="Auditability"></goal>
            <goal name="Private BC" mappedFeature="GDPR Compliance"></goal>
    </mapping>

<featureModel>
	<struct>
		<alt mandatory="true" name="Technical Solution">
			<alt name="Private Blockchain">
				<feature name="Tezos"/>
				<feature name="Hyperledger"/>
			</alt>
			<feature name="Public Blockchain"/>
		</alt>
	</struct>
        <constraints>
		<rule>
			<imp>
				<var>Auditability</var>
				<var>Tezos</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Public Blockchain</var>
				<not>
					<var>GDPR Compliance</var>
				</not>
			</imp>
		</rule>
	</constraints>
</featureModel>
</configuration>
